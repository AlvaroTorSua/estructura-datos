#include <iostream>
#include <string>
using namespace std;

int main()
{
    string nombre;
    int a,i;
    cout<<"escriba el nombre"<<endl;
    getline(cin,nombre);
    cout<<"escriba cuantas veces quiere imprimir"<<endl;
    cin>>a;
    for(i=0;i<a;i++){
    cout<<nombre<<"\n";
    }
    return 0;
}
